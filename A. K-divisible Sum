#include <bits/stdc++.h>
using namespace std;

using ll = long long;

void init_code() {
    ios_base::sync_with_stdio(false);
    cin.tie(NULL);
    cout.tie(NULL);
}

void always424() {
    ll n, k; cin >> n >> k;
    long long c = (n + k - 1) / k;
    k *= c;
    
    cout << (k + n - 1) / n << "\n";

}
int main() {
    init_code();
    int t; cin >> t;
    while(t--)
    always424();
}


